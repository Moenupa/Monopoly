      class hk.edu.polyu.comp.comp3211.monopoly.model.Player {
          - bankrupted : boolean
          - inJail : int
          - money : int
          - name : String
          - position : int
          - properties : Property[]
          + Player()
          + bankrupt()
          + getInJail()
          + getMoney()
          + getName()
          + getPosition()
          + goToJail()
          + isBankrupted()
          + move()
          + rollDice()
          + setInJail()
          + setMoney()
          + setName()
          + setPosition()
      }
    }
  }
  



