  namespace hk.edu.polyu.comp.comp3211.monopoly {
    namespace model {
      class hk.edu.polyu.comp.comp3211.monopoly.model.Board {
          - p_index : int
          - players : Player[]
          - round : int
          - squares : ISquare[]
